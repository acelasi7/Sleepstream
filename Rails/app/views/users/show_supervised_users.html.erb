<h1>Supervised Users (<%= @user.reverse_relationships.count %>)</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
      	<% if @user.reverse_relationships.any? %>
			<ol class="supervisors">
				<% @user.reverse_relationships.each do |reverse_relationship| %>
					<li><%= link_to User.find(reverse_relationship.followed_id).name, User.find(reverse_relationship.followed_id) %>
					</li>
				<% end %>
			</ol>
		<% end %>
  </div>
</div>
